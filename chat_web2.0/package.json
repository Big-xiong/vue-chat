    "scripts": {
        "test:unit": "jest --clearCache && vue-cli-service test:unit",
        "lint": "eslint --ext .js,.vue src",
        "build:dev": "vue-cli-service build --mode development",
        "build:prod:server": "vue-cli-service build --mode productionserver",
        "build:prod": "vue-cli-service build",
        "build:stage": "vue-cli-service build --mode staging",
        "dev": "vue-cli-service serve",
        "electron:build:devsellerwin": "vue-cli-service electron:build --mode developmentsellerwin",
        "electron:build:devserverwin": "vue-cli-service electron:build --mode developmentserverwin",
        "electron:build:devsellermac": "vue-cli-service electron:build --mode developmentsellermac",
        "electron:build:devservermac": "vue-cli-service electron:build --mode developmentservermac",
        "electron:build:sellerwin": "vue-cli-service electron:build --mode productionsellerwin",
        "electron:build:serverwin": "vue-cli-service electron:build --mode productionserverwin",
        "electron:build:sellermac": "vue-cli-service electron:build --mode productionsellermac",
        "electron:build:servermac": "vue-cli-service electron:build --mode productionservermac",
        "electron:serve:sellerwin": "vue-cli-service electron:serve --mode developmentsellerwin",
        "electron:serve:serverwin": "vue-cli-service electron:serve --mode developmentserverwin",
        "electron:serve:sellermac": "vue-cli-service electron:serve --mode developmentsellermac",
        "electron:serve:servermac": "vue-cli-service electron:serve --mode developmentservermac",
        "inspect:build": "vue-cli-service inspect --mode build-prod > webpack.prod.js",
        "inspect:dev": "vue-cli-service inspect --mode dev > webpack.dev.js",
        "postinstall": "electron-builder install-app-deps",
        "postuninstall": "electron-builder install-app-deps",
        "preview": "node build/index.js --preview",
        "prod": "vue-cli-service serve --mode production",
        "svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml",
        "test:ci": "npm run lint && npm run test:unit"
    },
